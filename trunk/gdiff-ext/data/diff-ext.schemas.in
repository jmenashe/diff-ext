<gconfschemafile>
    <schemalist>    
      <schema>
        <key>/schemas/apps/gdiff-ext/diff</key>
	<applyto>/apps/gdiff-ext/diff</applyto>
	<owner>gdiff-ext</owner>
	<type>string</type>
	<default>kdiff3</default>
	<locale name="C">
	  <short>diff command</short>
	  <long>
	    Command to execute when comparison is requested
	  </long>
	</locale>
      </schema>

      <schema>
        <key>/schemas/apps/gdiff-ext/diff3</key>
	<applyto>/apps/gdiff-ext/diff3</applyto>
	<owner>gdiff-ext</owner>
	<type>string</type>
	<default>kdiff3</default>
	<locale name="C">
	  <short>diff3 command</short>
	  <long>
	    Command to execute when 3-way comparison is requested
	  </long>
	</locale>
      </schema>

      <schema>
        <key>/schemas/apps/gdiff-ext/enable-diff3</key>
	<applyto>/apps/gdiff-ext/enable-diff3</applyto>
	<owner>gdiff-ext</owner>
	<type>bool</type>
	<default>false</default>
	<locale name="C">
	  <short>Enable 3-way diff</short>
	  <long>
	    Enable 3-way diff
	  </long>
	</locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/gdiff-ext/icons</key>
	<applyto>/apps/gdiff-ext/icons</applyto>
	<owner>gdiff-ext</owner>
	<type>string</type>
	<default>gdiff-ext</default>
	<locale name="C">
	  <short>icon theme</short>
	  <long>
	    icon theme
	  </long>
	</locale>
      </schema>
  </schemalist>
</gconfschemafile>
